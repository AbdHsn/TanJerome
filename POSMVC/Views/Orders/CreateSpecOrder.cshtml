                            <div class="form-row">
                                <div class="form-group">
                                    <label asp-for="SpectacleRx.Remark" class="control-label"></label>
                                    <textarea asp-for="SpectacleRx.Remark" id="SpectacleRx_Remark" class="form-control form-control-customized" rows="10" cols="85"></textarea>
                                </div>
