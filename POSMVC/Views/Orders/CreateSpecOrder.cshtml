                            </div>
                            <input class="form-control" type="text" data-provide="datepicker" data-date-autoclose="true" asp-for="Order.CollectionDate" value="">
                        </div>
                        <div class="clearfix pb-2 col-md-6">
                            <button type="button" id="btnPlaceOrder" class="btn btn-sm btn-outline-success float-sm-right float-md-right float-lg-right">
                                <i id="icoPlaceOrder" class="zmdi zmdi-check"></i>
                                Place Order & Payment Proceed
                            </button>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>
    <!-- #region Modal Section for Customer -->
    <div class="modal fade" id="mdlCreateForm" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" aria-modal="false">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title mt-0 font-16" id="myLargeModalLabel">Add New Customer</h5>
                    <button type="button" class="close" onclick='closeModalCreateForm("mdlCreateForm","CreateForm")'>
                        <span aria-hidden="true">×</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div id="CreateForm">
                        <div class="text-center">
                            <img src="~/ProjectImage/loading.gif" alt="Loading ..." />

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- #endregion -->

</div>

@section Scripts{
    <script src="~/js/bootstrap-datepicker.min.js"></script>
    <script src="~/js/searchable-dropdown/bootstrap-select.js"></script>
    <script type="text/javascript">

        //All Global Variables
        var countOrderItem = 0;
        var availableQty = 0;
        var cartList = new Array();
        //~All Global Variables

        //Use For Dropdown Search
        $('.selectpicker').selectpicker({
            liveSearch: true,
            maxOptions: 1
        });

        //Use For Pagination
        $(document).ready(function () {
            $('ul.pagination > li.disabled > a').addClass('page-link');
        });

        //Use For autocomplete search 
        $('#searchCustomer').autocomplete({
            minLength: 2,
            source: '/Orders/CustomerSearch',

            select: function (event, ui) {
                var serverData = getJsonData("Orders", "CustomerSearchResult", ui.item.label, "GET");
                //Set Customer Values From Search

                clearPrescription();
                cartList = new Array();
                toggleCartHeaderFooter(cartList);
                $('#cartPreview').empty();

                $('#Users_Id').val(serverData.users.id);
                $('#pdName').text("Name: " + serverData.personalDetail.name);
                $('#pdGender').text("Gender: " + serverData.personalDetail.gender);
                $('#pdAddress').text("Address: " + serverData.personalDetail.address);
                $('#pdIcNo').text("IC NO: " + serverData.personalDetail.icno);
                $('#pdMobileNo').text("Mobile: " + serverData.personalDetail.mobileNo);
                $('#pdEmail').text("Email: " + serverData.users.email);
            }
        });

        //Use For NR Calculator
        $('#btnNRCalculate').on("click", () => {
            $('#SpectacleRx_NearRsph').val(parseFloat($('#SpectacleRx_Nprsph').val()) + parseFloat($('#SpectacleRx_Npradd').val()));
            $('#SpectacleRx_NearRcyl').val($('#SpectacleRx_Nprcyl').val());
            $('#SpectacleRx_NearRaxis').val($('#SpectacleRx_Npraxis').val());
            $('#SpectacleRx_NearRva').val($('#SpectacleRx_Nprva').val());
        });

        //Use For NL Calculator
        $('#btnNLCalculate').on("click", () => {
            $('#SpectacleRx_NearLsph').val(parseFloat($('#SpectacleRx_Nplsph').val()) + parseFloat($('#SpectacleRx_Npladd').val()));
            $('#SpectacleRx_NearLcyl').val($('#SpectacleRx_Nplcyl').val());
            $('#SpectacleRx_NearLaxis').val($('#SpectacleRx_Nplaxis').val());
            $('#SpectacleRx_NearLva').val($('#SpectacleRx_Nplva').val());
        });

        //$('#btnNLCalculate').on("click", () => {
        //    clearItem();
        //});

        //Use for Get Last RX Value
        $('#btnLastRX').on("click", () => {
            var user = { id: $("#Users_Id").val() };
            if (user.id.length == 0) {
                showMessage("Customer selection is require.", "", "warning");
                return;
            }
            var serverData = getJsonData("Orders", "GetSpecLastRX", user, 'GET');

            if (serverData.length == 0 || serverData == null) {
                showMessage("No previous Spectacle RX found.", "", "info");
                return;
            }
            console.log(serverData);
            $('#btnLastRXClear').show();
            $('#SpectacleRx_Id').val(serverData.id);
            $('#SpectacleRx_Nprsph').val(serverData.nprsph);
            $('#SpectacleRx_Nprcyl').val(serverData.nprcyl);
            $('#SpectacleRx_Npraxis').val(serverData.npraxis);
            $('#SpectacleRx_Nprva').val(serverData.nprva);
            $('#SpectacleRx_Npradd').val(serverData.npradd);
            $('#SpectacleRx_Nplsph').val(serverData.nplsph);
            $('#SpectacleRx_Nplcyl').val(serverData.nplcyl);
            $('#SpectacleRx_Nplaxis').val(serverData.nplaxis);
            $('#SpectacleRx_Nplva').val(serverData.nplva);
            $('#SpectacleRx_Npladd').val(serverData.npladd);
            $('#SpectacleRx_NearRsph').val(serverData.nearRsph);
            $('#SpectacleRx_NearRcyl').val(serverData.nearRcyl);
            $('#SpectacleRx_NearRaxis').val(serverData.nearRaxis);
            $('#SpectacleRx_NearRva').val(serverData.nearRva);
            $('#SpectacleRx_NearRadd').val(serverData.nearRadd);
            $('#SpectacleRx_NearLsph').val(serverData.nearLsph);
            $('#SpectacleRx_NearLcyl').val(serverData.nearLcyl);
            $('#SpectacleRx_NearLaxis').val(serverData.nearLaxis);
            $('#SpectacleRx_NearLva').val(serverData.nearLva);
            $('#SpectacleRx_NearLadd').val(serverData.nearLadd);
            $('#SpectacleRx_Oprsph').val(serverData.oprsph);
            $('#SpectacleRx_Oprcyl').val(serverData.oprcyl);
            $('#SpectacleRx_Opraxis').val(serverData.opraxis);
            $('#SpectacleRx_Oprva').val(serverData.oprva);
            $('#SpectacleRx_Oplsph').val(serverData.oplsph);
            $('#SpectacleRx_Oplcyl').val(serverData.oplcyl);
            $('#SpectacleRx_Oplaxis').val(serverData.oplaxis);
            $('#SpectacleRx_Oplva').val(serverData.oplva);
            $('#SpectacleRx_PupilHeight').val(serverData.pupilHeight);
            $('#SpectacleRx_FrameSize').val(serverData.frameSize);
            $('#SpectacleRx_Adimension').val(serverData.adimension);
            $('#SpectacleRx_Bdimension').val(serverData.bdimension);
            $('#SpectacleRx_Rpd').val(serverData.rpd);
            $('#SpectacleRx_Lpd').val(serverData.lpd);
            $('#SpectacleRx_NearFarPd').val(serverData.nearFarPd);
            $('#SpectacleRx_Remark').val(serverData.remark);
        });

